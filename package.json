{
  "name": "vue-admin-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev ": "webpack-dev-server --config ./build/webpack.dev.conf.js",
    "build": "webpack --config ./build/webpack.prod.conf.js",
