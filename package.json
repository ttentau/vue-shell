    "html-webpack-plugin": "^4.5.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.5",
    "less": "^4.0.0",
    "less-loader": "^7.1.0",
    "vue-svg-icon": "^1.2.9",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^5.7.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
